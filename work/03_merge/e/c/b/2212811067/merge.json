98ef\u9762\u982d\u92ea\u5497\u9ed1\u9baa\u4e09\u500b\u5514\u540c\u5605\u90e8\u4f4d\uff0c\u5305\u62ec\u9ed1\u9baa\u8d64\u8eab\u3001\u8179\u8eab\u548c\u8525\u82b1\u9ed1\u9baa\u8179\u8eab\u9b5a\u8169\u84c9\uff0c\u4e00\u53e3\u6709\u4e09\u91cd\u9ed1\u9baa\u6ecb\u5473\uff0c\u65e2\u6709\u6cb9\u8102\u9999\u53c8\u6709\u8089\u611f\uff0c\u6b63\uff01\u81f3\u65bc\u53e6\u4e00\u6b3e\u4e09\u6587\u9b5a\u706b\u5c71 ($60)\uff0c\u9020\u578b\u4ea6\u975e\u5e38\u5438\u5f15\uff0c\u5e95\u90e8\u67092\u4ef6\u7d0b\u7406\u6f02\u4eae\u7684\u4e09\u6587\u9b5a\u58fd\u53f8\uff0c\u52a0\u4e0a\u9762\u982d\u5fae\u8fa3\u7684\u4e09\u6587\u9b5a\u84c9\u53ca\u5c71\u9802\u7684\u8fa3\u6912\u7d72\uff0c\u9bae\u8277\u596a\u76ee\uff0c\u8b93\u4eba\u80c3\u53e3\u5927\u589e\u3002\n\n\u610f\u7336\u672a\u76e1\uff0c\u6c7a\u5b9a\u518d\u9ede\u591a\u4e00\u5ba2\u7279\u5927\u6d77\u9c3b\u58fd\u53f8 ($40)\uff0c\u6d77\u9c3b\u8db3\u8db3\u6709\u6210 25cm \u9577\uff0c\u5916\u5f62\u5920\u66ec\u9707\u64bc\uff0c\u96d6\u7136\u8cea\u5730\u6bd4\u4e00\u822c\u9c3b\u9b5a\u8584\u8eab\uff0c\u4e0d\u7b97\u592a\u539a\u8089\uff0c\u4f46\u9b5a\u5473\u4e00\u6a23\u5481\u9bae\u751c\uff0c\u52a0\u4e0a\u4ee5\u84b2\u71d2\u65b9\u5f0f\u70ae\u88fd\uff0c\u4e00\u5927\u7247\u9c3b\u9b5a\u92ea\u5728\u58fd\u53f8\u98ef\u4e0a\uff0c\u98df\u5b8c\u771f\u4fc2\u6ecb\u5473\u5927\u6eff\u8db3\uff01\n\n\u5403\u7f77\u58fd\u53f8\u5c31\u8f2a\u5230\u8a66\u4e0b\u5e97\u88e1\u7684\u5929\u5a66\u7f85\uff0c\u5176\u5be6\u597d\u6709\u5e7e\u6b3e\u90fd\u5f88\u5438\u5f15\uff0c\u597d\u4f3c\u8edf\u6bbc\u87f9\u3001\u65e5\u672c\u7522\u756a\u85af\u540c\u5357\u74dc\u7b49\uff0c\u4e0d\u904e\u4eca\u6b21\u6211\u54cb\u5c31\u63c0\u5497\u70b8\u8766\u5929\u5a66\u7f85 ($40x2)\uff0c\u6bcf\u4eba\u4e00\u96bb\u5514\u4f7f\u722d\u3002\u5929\u5a66\u7f85\u5373\u53eb\u5373\u70b8\uff0c\u4e0a\u684c\u6642\u4ecd\u71b1\u8fa3\u8fa3\uff0c\u5916\u5c64\u70b8\u8863\u7565\u539a\uff0c\u4f46\u54ac\u4e0b\u4f9d\u7136\u9999\u8106\uff0c\u800c\u4e14\u8766\u8eab\u5de8\u5927\uff0c\u65b0\u9bae\u7d50\u5be6\u3002\n\n\u4e4b\u5f8c\u662f\u5bae\u5d0e\u7522\u71d2\u8178 ($40)\uff0c\u9999\u8178\u7522\u81ea\u5bae\u5d0e\uff0c\u76ae\u8106\u60f9\u5473\uff0c\u5165\u53e3\u9084\u53ef\u4ee5\u5403\u5230\u5c11\u5c11\u8edf\u9aa8\uff0c\u53e3\u611f\u8c50\u5bcc\u3002\u6700\u62b5\u8b9a\u5c31\u4fc2\u4e0a\u684c\u6642\u9084\u914d\u6709\u751c\u7f8e\u7684\u7c9f\u7c73\u7c92\u548c\u7206\u6c41\u8eca\u5398\u8304\uff0c\u7d55\u5c0d\u662f\u8aa0\u610f\u4e4b\u4f5c\u3002\n\n\u53e6\u5916\u5462\u6b3e\u5510\u63da\u70b8\u96de ($20) \u4ea6\u975e\u5e38\u503c\u5f97\u63a8\u4ecb\uff0c\u70b8\u96de\u5373\u53eb\u5373\u70b8\uff0c\u6240\u4ee5\u7aef\u4e0a\u4f86\u6642\u4ecd\u7136\u975e\u5e38\u8106\u53e3\uff0c\u8089\u8cea\u5ae9\u6ed1\u53c8\u6709\u8089\u6c41\uff0c\u5403\u6642\u53ef\u8638\u5c11\u8a31\u6c99\u5f8b\u91ac\uff0c\u5165\u53e3\u66f4\u70ba\u60f9\u5473\u3002\u70b8\u96de\u4e00\u4efd\u67093\u4ef6\uff0c\u975e\u5e38\u62b5\u98df\uff0c\u5c6c\u5fc5\u9ede\u4e4b\u9078\u3002\n\n\u5728\u4e09\u6b3e\u934b\u7269\u4e2d\u9078\u4e86A4\u548c\u725b\u58fd\u559c\u71d2 ($60)\uff0c\u58fd\u559c\u71d2\u9078\u7528\u4e86\u65e5\u672c\u9e7f\u5152\u5cf6A4\u7d1a\u548c\u725b\u88fd\u4f5c\uff0c\u8089\u5473\u975e\u5e38\u9999\u6fc3\uff0c\u52a0\u4e0a\u725b\u8102\u4ee4\u6e6f\u5e95\u5e36\u6709\u725b\u9999\uff0c\u5165\u53e3\u60f9\u5473\u9999\u751c\u3002\u81f3\u65bc\u914d\u6599\u65b9\u5c31\u6709\u51ac\u83c7\u3001\u5927\u8525\u3001\u5a03\u5a03\u83dc\u540c\u7c89\u7d72\u7b49\uff0c\u7528\u6599\u5341\u8db3\uff0c\u4ee5 $60 \u4e00\u934b\u5462\u500b\u50f9\u9322\u771f\u4fc2\u8d85\u62b5\u98df\u3002\n\n\u5e97\u88e1\u7684\u751c\u54c1\u53ea\u6709\u4e00\u6b3e\uff0c\u5c31\u4fc2\u6d77\u9e7d\u96ea\u7cd5 ($20)\uff0c\u4e00\u5834\u569f\u5230\u7576\u7136\u8981\u8a66\u4e0b\u3002\u676f\u6d77\u9e7d\u96ea\u7cd5\u8cea\u5730\u975e\u5e38\u9806\u6ed1\uff0c\u5165\u53e3\u4e0d\u4f46\u5e36\u6fc3\u90c1\u725b\u5976\u9999\uff0c\u800c\u4e14\u98df\u5230\u5c3e\u6bb5\u66f4\u6703\u6ef2\u51fa\u9663\u9663\u6d77\u9e7d\u7684\u5fae\u9e79\uff0c\u914d\u57cb\u9802\u90e8\u7684\u7d05\u8c46\u84c9\u5403\uff0c\u96d9\u91cd\u6ecb\u5473\uff0c\u771f\u4fc2\u4ee4\u4eba\u56de\u5473\u7121\u7aae\u3002\n\n\u5e36\u9806\u4e00\u63d0\uff0c\u9910\u5ef3\u8a2d\u6700\u4f4e\u6d88\u8cbb\uff0c\u665a\u5e02\u6642\u6bb5\uff0c\u6bcf\u4eba\u70ba $100\uff0c\u5305\u62ec\u52a0\u4e00\u3002",
